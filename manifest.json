{
  "name": "SaveTabs",
  "manifest_version": 2,
  "version": "1.1.0",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {
    "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "downloads"
  ]
}
