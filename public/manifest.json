{
  "name": "__MSG_extension_name__",
  "short_name": "__MSG_extension_short_name__",
  "manifest_version": 2,
  "version": "4.0.0",
  "author": "胡镇华",
  "description": "__MSG_extension_description__",
  "default_locale": "en",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon-128.png",
    "default_popup": "savetabs.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "options_page": "options.html",
  "offline_enabled": true,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "mac": "Command+Shift+Space",
        "windows": "Alt+Shift+Space",
        "linux": "Alt+Shift+Space"
      }
    },
    "open_workspace_all": {
      "description": "open_workspace_all"
    },
    "open_workspace_window": {
      "description": "open_workspace_window"
    },
    "open_workspace_note": {
      "description": "open_workspace_note"
    },
    "open_workspace_temporary": {
      "description": "open_workspace_temporary"
    },
    "open_workspace_tab": {
      "description": "open_workspace_tab"
    },
    "open_workspace_bookmark": {
      "description": "open_workspace_bookmark"
    },
    "open_workspace_history": {
      "description": "open_workspace_history"
    },
    "add_window": {
      "description": "add_window"
    },
    "add_note": {
      "description": "add_note"
    },
    "add_temporary": {
      "description": "add_temporary"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/",
    "downloads",
    "history",
    "bookmarks",
    "\u003Call_urls>"
  ],
  "content_security_policy": "script-src 'self' 'sha256-4RS22DYeB7U14dra4KcQYxmwt5HkOInieXK1NUMBmQI='; object-src 'self'",
  "web_accessible_resources": [
    "savetabs.html",
    "options.html"
  ]
}
